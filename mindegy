            this.matrix = new int[mandatum, partszam];
        }
        public void matrixFel()
        {
            for(int i = 0; i < matrix.GetLength(0); i++)
            {
                for(int j = 0; j < matrix.GetLength(1); j++)
                {
                    double ertek = szavazatszam[j] / i + 1;
                    if()
                }
            }
        }

    }
